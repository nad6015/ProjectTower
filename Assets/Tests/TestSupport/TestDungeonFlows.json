{
  "exploreFloor": {
    "baseDungeon": ["start", "explore", "combat", "explore", "end"],
    "flows": [
      {
        "matches": ["restPoint", "explore"],
        "replacer": ["explore", "combat"]
      },
      {
        "matches": ["explore", "combat"],
        "replacer": ["combat", "explore", "combat", "treasure"]
      },
      {
        "matches": ["combat", "explore"],
        "replacer": ["explore", "restPoint"]
      }
    ]
  },
  "FindKey": {
    "baseDungeon": ["start", "explore", "combat", "lock", "end"],
    "flows": [
      {
        "matches": ["combat", "lock"],
        "replacer": ["lock", "combat", "combat", "restPoint"]
      },
      {
        "matches": ["lock"],
        "replacer": ["lock", "treasure", "combat"]
      },
      {
        "matches": ["combat"],
        "replacer": ["explore", "explore", "combat"]
      }
    ]
  },
  "FightBoss": {
    "baseDungeon": ["start", "combat", "explore", "end"],
    "flows": [
      {
        "matches": ["combat"],
        "replacer": ["restPoint", "treasure", "combat", "explore"]
      }
    ]
  }
}
