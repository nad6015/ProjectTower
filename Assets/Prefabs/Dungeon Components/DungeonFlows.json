{
  "exploreFloor": {
    "baseDungeon": ["start", "explore", "combat", "treasure", "end"],
    "flows": [
      {
        "matches": ["explore", "combat"],
        "replacer": ["combat", "explore", "combat", "restPoint"]
      },
      {
        "matches": ["restPoint", "generic"],
        "replacer": ["generic", "combat"]
      },
      {
        "matches": ["explore", "combat"],
        "replacer": ["combat", "generic", "combat", "explore"]
      },
      {
        "matches": ["combat", "generic"],
        "replacer": ["explore", "restPoint"]
      }
    ]
  },
  "FindKey": {
    "baseDungeon": ["start", "explore", "combat", "generic", "end"],
    "flows": [
      {
        "matches": ["explore", "combat"],
        "replacer": ["lock", "combat"]
      },
      {
        "matches": ["generic"],
        "replacer": ["combat", "restPoint", "explore"]
      },
      {
        "matches": ["combat"],
        "replacer": ["generic", "explore", "combat"]
      }
    ]
  },
  "FightBoss": {
    "baseDungeon": ["start", "explore", "combat", "generic", "end"],
    "flows": [
      {
        "matches": ["combat"],
        "replacer": ["explore", "restPoint", "boss"]
      }
    ]
  }
}
