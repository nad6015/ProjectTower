{
  "exploreFloor": {
    "baseDungeon": ["start", "explore", "combat", "item", "end"],
    "flows": [
      {
        "matches": ["explore", "combat"],
        "replacer": ["combat", "explore", "combat", "restPoint"]
      },
      {
        "matches": ["restPoint", "item"],
        "replacer": ["item", "combat"]
      },
      {
        "matches": ["explore", "combat"],
        "replacer": ["combat", "item", "combat", "explore"]
      },
      {
        "matches": ["combat", "item"],
        "replacer": ["explore", "restPoint"]
      }
    ]
  },
  "FindKey": {
    "baseDungeon": ["start", "explore", "combat", "item", "end"],
    "flows": [
      {
        "matches": ["explore", "combat"],
        "replacer": ["lock", "combat", "item"]
      }
    ]
  },
  "FightBoss": {
    "baseDungeon": ["start", "explore", "combat", "item", "end"],
    "flows": [
      {
        "matches": ["combat"],
        "replacer": ["explore", "restPoint", "boss"]
      }
    ]
  }
}
